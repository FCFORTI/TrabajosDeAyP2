package Guía3Ejercicio4;
import java.util.Comparator;

public class ComparadorDeDocentes implements Comparator<Sueldo>{
	
	private Sueldo sueldo;
	
	@Override
	public int compareTo(Sueldo sueldo1) {
		if(this.sueldo.calcularSueldo() < sueldo1.calcularSueldo()){
			return -1;
		} else if(this.sueldo.calcularSueldo() > sueldo1.calcularSueldo()){
			return 1;
		}
		return 0;
	}

}
